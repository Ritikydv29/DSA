// Print the array in reverse.
#include<iostream>
using namespace std;
void solve(int arr[],int n)
{

    if(n<0)
    {
        return;
    }
   
    cout<<" "<<arr[n];
     solve(arr,n-1);
    
    
}
int main()
{
    int n=0;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    solve(arr,n-1);
    return 0;
}
